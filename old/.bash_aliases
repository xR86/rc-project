alias ea='gedit ~/.bash_aliases; source ~/.bash_aliases && source $HOME/.bash_aliases && echo "aliases sourced --ok."'
alias silent_ea='source ~/.bash_aliases && source $HOME/.bash_aliases && echo "[silent]aliases sourced --ok."'
alias rcproj-s='cd /home/xr-86/httpreq/server/'
alias rcproj-c='cd /home/xr-86/httpreq/client/'

